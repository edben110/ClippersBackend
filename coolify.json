{
  "name": "Clipers Backend",
  "description": "Spring Boot API for Clipers - Video CV Social Network",
  "type": "dockerfile",
  "dockerfile": "Dockerfile",
  "port": 8080,
  "healthcheck": {
    "enabled": true,
    "path": "/actuator/health",
    "interval": 30,
    "timeout": 10,
    "retries": 3,
    "start_period": 60
  },
  "build_args": {},
  "volumes": [
    {
      "name": "uploads",
      "mount_path": "/app/uploads",
      "size": "10Gi"
    }
  ],
  "resources": {
    "memory": "1Gi",
    "cpu": "1",
    "memory_reservation": "512Mi"
  }
}